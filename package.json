  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@okta/okta-auth-js": "v5.1.1",
    "@okta/okta-react": "^6.1.0",
    "@okta/okta-signin-widget": "^5.10.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
